<mvc:View controllerName="sap-photo-book.controller.Main" xmlns:html="http://www.w3.org/1999/xhtml" xmlns:mvc="sap.ui.core.mvc"
	displayBlock="true" xmlns="sap.m">
	<App>
		<pages>
			<Page title="{i18n>title}">
				<content>
					<TileContainer id="container" tileDelete="handleTileDelete" tiles="{/TileCollection}">
						<CustomTile class="ui-draggable ui-droppable sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout">
							<content>
								<ImageContent src="{content}" width="100%" height="100%"/>
							</content>
						</CustomTile>
					</TileContainer>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>